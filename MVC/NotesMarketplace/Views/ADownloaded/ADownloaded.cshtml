@model IEnumerable<NotesMarketplace.context.ADownloadedModel>
@{
    ViewBag.Title = "ADownloaded";
    Layout = "~/Views/Shared/_mytemplate2(admin).cshtml";
}

<section class="admin">
    <div class="content-box-md">
        <div class="container1">
            <h1 style="margin-top: 100px">Downloaded Notes</h1>
            <div class="er">
                <p>Note</p>
                <p>Seller</p>

                <p>Buyer</p>
            </div>
            <div class="search-flex">
                <div class="add-admin-btn">
                    <div class="er1">
                        

                                <select id="regionDropdown">
                                    <option value="All">Select Note</option>
                                    <option value="Demo Title">Demo Title</option>
                                    <option value="Demo Title 2">Demo Title 2</option>
                                    
                                </select>

                                <select id="roleDropdown">
                                    <option value="All">Select Seller</option>
                                    <option value="KhyatiPatel">KhyatiPatel</option>
                                    <option value="RahulShah">RahulShah</option>
                                    
                                </select>

                                <select id="associateStatusDropdown">
                                    <option value="All">Select Buyer</option>
                                    <option value="RahulShah">RahulShah</option>
                                    <option value="SumanTrivedi">SumanTrivedi</option>
                                    <option value="">buyer3</option>
                                </select>
                            </div>
                            </div>
                       
                        <div class="flex-search-buttons">
                            <input type="text" id="myInput" onkeyup="myFunction()" class="search" name="search" placeholder="&emsp;&emsp;Search" style="background-image: url('../admin/images/search.jpg');
                              background-position: 10px 10px; background-repeat: no-repeat">

                            <button type="button">SEARCH</button>
                        </div>

                    </div>
        </div>
    </div>
</section>

<div style="overflow-x: auto;">
    <div class="context-menu-container" id="context-menu-items">
        <ul>
            <li>Download Notes</li>
            <li>View More Details</li>
           
        </ul>
    </div>
    <table class="main-table" border="1" width="98%" id="myTable">
        <tr class="line">
            <th>SR NO.</th>
            <th>NOTE TITLE</th>
            <th>CATEGORY</th>
            <th>BUYER</th>
            <th></th>
            <th>SELLER</th>
            <th></th>
            <th>SELL TYPE</th>
            <th>PRICE</th>
            <th>DOWNLOADE DATE/TIME</th>
            <th></th>
        </tr>
        <tbody id="download-forms-table-tbody">
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.download.ID</td>
                    <td>@item.download.NoteTitle</td>
                    <td>@item.download.NoteCategory</td>
                    <td>@item.user.FullName</td>
                    <td><img src="@Url.Content("~/Content/mytemplate/")Marketplace/front/images/eye.png" /></td>
                    <td>@item.user1.FullName</td>
                    <td><img src="@Url.Content("~/Content/mytemplate/")Marketplace/front/images/eye.png" /></td>
                    <td>@item.download.IsPaid</td>
                    <td>@item.download.PurchasedPrice</td>
                    <td>@item.download.CreatedDate</td>
                    <td class="context-menu" data-container-id="context-menu-items" data-row-id="1"></td>
                </tr>
            }
        </tbody>
    </table>
</div>



